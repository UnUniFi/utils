{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "jpyx-website": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/jpyx-website",
      "sourceRoot": "projects/jpyx-website/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/jpyx-website",
            "index": "projects/jpyx-website/src/index.html",
            "main": "projects/jpyx-website/src/main.ts",
            "polyfills": "projects/jpyx-website/src/polyfills.ts",
            "tsConfig": "projects/jpyx-website/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/jpyx-website/src/favicon.ico",
              "projects/jpyx-website/src/assets"
            ],
            "styles": ["projects/jpyx-website/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/jpyx-website/src/environments/environment.ts",
                  "with": "projects/jpyx-website/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "jpyx-website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "jpyx-website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jpyx-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jpyx-website/src/test.ts",
            "polyfills": "projects/jpyx-website/src/polyfills.ts",
            "tsConfig": "projects/jpyx-website/tsconfig.spec.json",
            "karmaConfig": "projects/jpyx-website/karma.conf.js",
            "assets": [
              "projects/jpyx-website/src/favicon.ico",
              "projects/jpyx-website/src/assets"
            ],
            "styles": ["projects/jpyx-website/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/jpyx-website/tsconfig.app.json",
              "projects/jpyx-website/tsconfig.spec.json",
              "projects/jpyx-website/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/jpyx-website/e2e/protractor.conf.js",
            "devServerTarget": "jpyx-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jpyx-website:serve:production"
            }
          }
        }
      }
    },
