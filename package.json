    "bundle:eurx": "docker build . -f ./projects/telescope-extension/editions/eurx/Dockerfile -t lcnem/telescope-eurx",
    "bundle:pricefeed": "docker build ./projects/pricefeed/ -f ./projects/pricefeed/Dockerfile -t lcnem/botany-pricefeed",
    "prepare": "husky install"
  },
  "private": true,
